{
  "name": "pfe/rh",
  "description": "A project for managing employee data, including leave requests, overtime hours, and financial records.",
  "type": "project",
  "require": {
    "php": "^7.4",
    "monolog/monolog": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "src/includes/functions.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "scripts": {
    "test": "phpunit",
    "start": "php -S localhost:8000 -t src/public"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}